{
    "editor.fontSize": 12,
    "editor.fontFamily": "JetBrains Mono NL, Hack, Noto Sans Mono CJK SC, Noto Mono, Consolas, monospace",
    "editor.fontLigatures": false,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "on",
    "editor.maxTokenizationLineLength": 4294967296,
    "editor.tokenColorCustomizations": {
        "comments": {
            "fontStyle": "bold",
            "foreground": "#999999"
        },
        "keywords": {
            "fontStyle": "bold",
            "foreground": "#ff5500"
        },
        "functions": {
            "fontStyle": "bold",
            "foreground": "#0000ff"
        },
        "variables": "#dd55aa",
        "strings": "#00aa00",
        "numbers": "#aa0000"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "workbench.colorCustomizations": {
        "editor.selectionHighlightBackground": "#ffff55",
        "editorBracketHighlight.foreground1": "#00cc00",
        "editorBracketHighlight.foreground2": "#33ccff",
        "editorBracketHighlight.foreground3": "#aa00aa",
        "editorBracketHighlight.foreground4": "#44ccaa",
        "editorBracketHighlight.foreground5": "#4444bb",
        "editorBracketHighlight.foreground6": "#8080ff",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000"
    },
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Default Light+",
    "debug.console.fontSize": 12,
    "debug.onTaskErrors": "showErrors",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "args": [
                "-NoLogo"
            ],
            "overrideName": true
        }
    },
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.inheritEnv": false,
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "search.useReplacePreview": false,
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 0,
    "files.associations": {
        "*.json": "jsonc"
    },
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "markdown.preview.fontFamily": "'Noto Sans CJK SC', 'Noto Sans', sans-serif",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-lualatex",
                "-bibtex",
                "-synctex=1",
                "--shell-escape",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ]
        },
        {
            "name": "clean",
            "command": "latexmk",
            "args": [
                "-c",
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "clean",
            "tools": [
                "clean"
            ]
        }
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.idx",
        "*.ind",
        "*.ist",
        "*.lof",
        "*.log",
        "*.lot",
        "*.nav",
        "*.out",
        "*.snm",
        "*.toc",
        "*.run.xml"
    ],
    "latex-workshop.bind.enter.key": false,
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.view.pdf.tab.openDelay": 0,
    "latex-workshop.view.pdf.viewer": "tab",
    "python.linting.pylintArgs": [
        "--generate-members"
    ],
    "python.formatting.autopep8Args": [
        "--max-line-length=65536"
    ],
    "jupyter.themeMatplotlibPlots": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "latex-workshop.intellisense.argumentHint.enabled": false
}
